{
  "manifest_version": 3,
  "name": "YgoProDeck Pack Filter",
  "version": "1.0",
  "description": "Filter out Duel Links and Master Duel packs ygoprodeck.com/pack-sim",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["thirdParty/jquery-3.6.4.min.js", "scripts/content.js"],
      "matches": [
        "https://ygoprodeck.com/*"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}